# Next.js environment variables for Questly
# Copy this file to .env.local and fill in secrets for local development.
# Example (PowerShell): Copy-Item .env.example .env.local

# --- OpenAI (optional, used if you wire real chat responses) ---
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
MODEL_NAME=gpt-4o-mini
# Model/runtime tuning
TEMPERATURE_EXPLORE=0.8
TEMPERATURE_QUIZ=0.2
MAX_TOKENS_CHAT=700
MAX_TOKENS_SESSION=3500

# --- Supabase (optional) ---
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# --- Analytics (optional) ---
# Typically configured by the hosting platform (e.g., Vercel). Left for reference.
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=
